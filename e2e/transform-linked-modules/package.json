{
  "jest": {
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__tests__",
      "<rootDir>/ignored/",
      "jest-jasmine2",
      "jest-each",
      "jest-circus"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/preprocessor.js"
    }
  }
}
